{
    "_version": "1.12.0",
    "sap.app": {
        "id": "Japra",
        "type": "application",
        "i18n": "i18n/i18n_en.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
      }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "Japra.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.60",
            "libs": {
                "sap.ui.core": {},
				"sap.tnt": {},
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "Japra.i18n.i18n_en",
                    "supportedLocales": [""],
                    "fallbackLocale": ""
                }
            }
        },
        "resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "Japra.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "home",
                    "target": "home"
                },
                {
                    "pattern": "home",
                    "name": "home",
                    "target": "home"
                },
                {
                    "pattern": "kanji",
                    "name": "kanji",
                    "target": "kanji"
                },
                {
                    "pattern": "vocabulary",
                    "name": "vocabulary",
                    "target": "Vocabulary"
                }
            ],
            "targets": {
                "home": {
                    "viewId": "home",
                    "viewName": "Home"
                },
                "kanji": {
                    "viewId": "kanji",
                    "viewName": "Kanji"
                },
                "vocabulary": {
                    "viewId": "vocabulary",
                    "viewName": "Vocabulary"
                }
            }
        },
        "contentDensities": {
            "compact": false,
            "cozy": false
        }
    }
  }